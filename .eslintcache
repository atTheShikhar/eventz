=\"md\">\n                <Grid container spacing={4} className={classes.parent}>\n                    <Grid item xs={12} md={6}>\n                        <img className={classes.image} src={aboutUsImg} alt=\"Man reading texts\"/>\n                    </Grid>\n\n                    <Grid item xs={12} md={6} className={classes.content}>\n                        <h1 className={classes.heading}>\n                            About Us<span style={{ color: \"#2e4390\"}}>.</span>\n                        </h1>\n\n                        <Divider className={classes.vmargin} variant=\"middle\"/>\n\n                        <p className={classes.para}>\n                            <strong style={{ color: \"#2e4390\"}}>Eventz</strong> is an event management webapp built specifically\n                           for creating and booking professional events \n                           like Seminars, Conferences, Workshops etc.\n                           <br/>\n                           <br/>\n                           Organisers can give us their event details and we handle the \n                           tickets generation/verification, attendant information, revenue, etc. for them. \n                           We believe that organisers should not be charged for \n                           events that they are organising for free. Our service only charges you \n                           for the events that are paid, free events are managed for free.\n                           <br/>\n                           <br/>\n                           People can search events based on their interests and book tickets without any hassle.\n                           Tickets are in the form of QR code that can be scanned at the event venue entry.\n                        </p>\n                    </Grid>\n                </Grid>\n                <Box mt={7}>\n                    <Copyright />\n                </Box>\n            </Container>\n        </div>\n    )\n}\n\nexport default About\n","/home/s123/Desktop/project/eventzClient/src/pages/FormStyle.js",[],"/home/s123/Desktop/project/eventzClient/src/components/Copyright.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/AddressDetails.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/Styles.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/OrganiserDetails.js",[],"/home/s123/Desktop/project/eventzClient/src/components/buttons/SubmitButton.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/auth.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/EventDetails.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/validators.js",[],"/home/s123/Desktop/project/eventzClient/src/routes/PrivateRoute.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Activate.js",["336"],"import React, { useEffect, useState } from 'react'\nimport jwt_decode from 'jwt-decode';\nimport { Button, Paper, Container, Divider, Typography } from '@material-ui/core';\nimport useStyles from './FormStyle';\nimport StatusAndRedirect from '../components/StatusAndRedirect';\nimport { activateAccount } from '../helpers/auth';\n\n\nfunction Activate({match,history}) {\n    //Hooks\n    const classes = useStyles();\n    const [formData,setFormData] = useState({\n        name:\"\",\n        token: \"\",\n        show: \"activate\"\n    });\n    useEffect(() => {\n        const token =  match.params.token;\n        try {\n            const decoded = jwt_decode(token);\n            setFormData({\n                ...formData,\n                name: decoded.fname,\n                token\n            })\n        } catch(err) {\n            setFormData({\n                ...formData,\n                show: \"failed\"\n            })\n        }\n    },[])\n    //Functions \n    const confirmAccount = (e) => {\n        e.preventDefault();\n        //post handler\n        activateAccount(formData,setFormData,history);\n    }\n\n\n    //Render Logic\n    switch(formData.show) {\n        case \"activate\": return (\n            <div className={classes.page}>\n                <Container maxWidth=\"sm\">\n                    <Paper className={classes.card}>\n                        <Container className={classes.container}>\n                            <Typography\n                                variant=\"h4\"\n                                align=\"left\"\n                                color=\"textSecondary\"\n                            >\n                                Welcome <span className={classes.primaryColor}>\n                                    {formData.name}\n                                </span>,\n                            </Typography>\n                        </Container>\n                        <Container>\n                            <Typography\n                                align=\"left\"\n                                color=\"textSecondary\"\n                            >\n                                Please click the button below to confirm your account.\n                            </Typography>\n                            <Divider variant=\"middle\" />\n                        </Container>\n                        <Container className={classes.container}>\n                            <Button\n                                fullWidth\n                                variant=\"contained\"\n                                color=\"inherit\"\n                                onClick={confirmAccount}\n                                size=\"large\"\n                                className={classes.submit}\n                                style={{\n                                    backgroundColor: \"#20232a\",\n                                    color: \"white\"\n                                }}\n                            >\n                                Confirm Account\n                            </Button>\n                        </Container>\n                    </Paper>\n                </Container>\n            </div>\n        )\n        case \"failed\": return (\n            <StatusAndRedirect \n                header=\"Activation Failed\"\n                message=\"Link expired or invalid link!\"\n                type=\"failed\"\n            />\n        )\n        case \"success\": return (\n            <StatusAndRedirect \n                header=\"Activation Successful\"\n                message=\"Now you can login with your account!\"\n                type=\"success\"\n            />\n        )\n        case \"already exists\": return (\n            <StatusAndRedirect\n                header=\"Confirmed Already\"\n                message=\"Email already confirmed!\"\n                type=\"failed\"\n            />\n        )\n        default: return (<div></div>)\n    }\n}\n\nexport default Activate\n","/home/s123/Desktop/project/eventzClient/src/components/StatusAndRedirect.js",["337"],"import React, { useEffect, useState } from 'react'\nimport { Container, Typography, Card } from '@material-ui/core';\nimport useStyles from '../pages/FormStyle';\nimport { useHistory } from 'react-router-dom';\n\nfunction StatusAndRedirect(props) {\n    //Hooks\n    const classes = useStyles();\n    const history = useHistory();\n    const [seconds,setSeconds] = useState(5);\n    useEffect(() => {\n        const interval = setInterval(() => {\n            setSeconds((seconds) => seconds - 1)\n        }, 1000);\n\n        setTimeout(() => {\n            clearInterval(interval);\n            history.push('/');\n        },5000);\n    },[])\n\n    //Attributes\n    const { header, message, type } = props;\n    let headerColor, bgColor;\n    if (type === \"failed\") {\n        headerColor = classes.errorColor;\n        bgColor = classes.errorBg;\n    } else if(type === \"success\") {\n        headerColor = classes.successColor;\n        bgColor = classes.successBg;\n    } else {\n        headerColor = \"\";\n        bgColor = \"\";\n    }\n\n    //Render Logic\n    return (\n        <div className={classes.page}>\n            <Container maxWidth=\"sm\">\n                <Card className={`${classes.card} ${bgColor}`}>\n                    <Container className={classes.container}>\n                        <Typography\n                            variant=\"h4\"\n                            align=\"center\"\n                            className={headerColor}\n                        >\n                            {header}\n                        </Typography>\n                    </Container>\n                    <Container className={classes.container}>\n                        <Typography\n                            align=\"center\"\n                            color=\"textPrimary\"\n                        >\n                            {message}\n                        </Typography>\n                        <br/>\n                        <Typography\n                            align=\"center\"\n                            color=\"textPrimary\"\n                            variant=\"subtitle2\"\n                        >\n                            Redirecting to Homepage in {seconds} seconds...\n                        </Typography>\n                    </Container>\n                </Card>\n            </Container>\n        </div >\n    )\n}\n\nexport default StatusAndRedirect;\n","/home/s123/Desktop/project/eventzClient/src/pages/ForgetPassword.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/ResetPassword.js",[],"/home/s123/Desktop/project/eventzClient/src/components/NetError.js",[],"/home/s123/Desktop/project/eventzClient/src/components/feedback/snackbar.js",[],"/home/s123/Desktop/project/eventzClient/src/context/GlobalState.js",[],"/home/s123/Desktop/project/eventzClient/src/routes/PublicRoute.js",[],"/home/s123/Desktop/project/eventzClient/src/context/Context.js",[],"/home/s123/Desktop/project/eventzClient/src/context/ComponentState.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/createEvent.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/BrowseEvents.js",["338"],"import { Container, Divider, Grid, makeStyles} from '@material-ui/core'\nimport { Pagination, PaginationItem } from '@material-ui/lab'\nimport React, { useContext, useEffect, useState } from 'react'\nimport { useHistory, useLocation } from 'react-router'\nimport { Link } from 'react-router-dom'\nimport EventCard from '../components/cards/EventCard'\nimport NotFound from '../components/NotFound'\nimport { ComponentContext, DataContext } from '../context/Context'\nimport { fetchData } from '../helpers/fetchData'\n\nconst useStyles = makeStyles((theme) => ({\n    bgGrey: {\n        backgroundColor: \"#fafafa\"\n    },\n    vpadding: {\n        paddingTop: \"30px\",\n        paddingBottom: \"30px\"\n    },\n    vmargin: {\n        marginTop: \"30px\",\n        marginBottom: \"30px\"\n    },\n    pagination: {\n        \"&.MuiPagination-root .Mui-selected\": {\n            color: \"white\",\n            backgroundColor: \"#20232a\"\n        }\n    },\n    divContainer: {\n        display: \"flex\",\n        flexDirection: \"row\",\n        justifyContent: \"center\",\n    }\n}))\n\nfunction BrowseEvents(props) {\n    const classes = useStyles();\n\n    //Gets query parameters from the link\n    //This just gets the page num, \n    //actual search query comes from passed state with history.push() by search button\n    const { search } = useLocation(); \n\n    const {setFeedback,load,setLoad} = useContext(ComponentContext);\n    const history = useHistory();\n    const {events,setEvents} = useContext(DataContext);\n    const [pageData,setPageData] = useState({});\n\n    useEffect(() => {\n        const url = `/api/get-events${search}`;\n        async function fetchEvents() {\n            const data = await fetchData(url,setFeedback,history);\n            // console.log(data);\n            setEvents(data.events);\n            setPageData({\n                currentPage: data.currentPage,\n                totalPages: data.totalPages,\n                eventCount: data.eventCount\n            })\n        }\n        fetchEvents();\n        return function() {\n            setEvents(null);\n        }\n    },[load])\n\n    return (\n        <div className={classes.bgGrey}>\n        <Container maxWidth=\"lg\" className={classes.vpadding}>\n            {\n                (events!==null && events?.length !== 0) ? \n                (\n                    <>\n                        <Grid container spacing={2} >\n                            {\n                                events.map((item) => {\n                                    return (\n                                        <Grid \n                                            key={item._id} \n                                            item \n                                            xs={12} \n                                            sm={6} \n                                            md={4} \n                                        >\n                                            <EventCard\n                                                eventData={item}\n                                                clickRoute={`/event/${item._id}`}\n                                            />\n                                        </Grid>\n                                    );\n                                })\n                            }\n                        </Grid>\n\n                        <Divider variant=\"middle\" className={classes.vmargin}/>\n\n                        <div className={classes.divContainer}>\n                            <Pagination \n                                className={classes.pagination}\n                                count={pageData.totalPages}      \n                                onChange={(e,page) => {\n                                    setLoad(load => !load);\n                                }}\n                                renderItem={(item) => (\n                                    <PaginationItem\n                                        component={Link}\n                                        to={`/browse?page=${item.page}`}\n                                        {...item}\n                                    />\n                                )}\n                                shape=\"rounded\"\n                                size=\"large\"\n                            />\n                        </div>\n                    </>\n                ) :\n                (\n                    <NotFound/>\n                )\n            }\n        </Container>\n        </div>\n    )\n}\n\nexport default BrowseEvents\n","/home/s123/Desktop/project/eventzClient/src/components/navbar/Navbar.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/useStyles.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/NavList.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/menuItems.js",[],"/home/s123/Desktop/project/eventzClient/src/components/inputs/Textbox.js",[],"/home/s123/Desktop/project/eventzClient/src/components/cards/EventCard.js",["339"],"import { \n    Card, \n    CardActionArea,\n    CardActions, \n    CardContent, \n    CardMedia, \n    Chip, \n    Divider, \n    Typography,\n    Grid\n} from '@material-ui/core'\nimport LocationOnIcon from '@material-ui/icons/LocationOn';\nimport EventIcon from '@material-ui/icons/Event';\nimport ScheduleIcon from '@material-ui/icons/Schedule';\nimport React,{useState,useEffect} from 'react'\nimport noImage from '../../assets/no-image.jpg'\nimport useStyles from './CardStyle'\nimport { useHistory } from 'react-router';\nimport ProgressBar from '../dataDisplay/ProgressBar';\n\nfunction EventCard(props) {\n    const { title,genre,dateAndTime,isFree,price } = props.eventData.eventDetails;\n    const { stateName } = props.eventData.eventAddress;\n    const { imageLocation,totalTickets,bookedTickets } = props.eventData;\n    const { clickRoute } = props;\n    const date = new Date(dateAndTime).toDateString();\n    const time = new Date(dateAndTime).toLocaleTimeString();\n    let heading = title;\n    if(heading.length >= 20) {\n        heading = heading.substring(0,19) + \"...\";\n    }\n\n    const classes = useStyles();\n    const history = useHistory();\n    const [image,setImage] = useState(noImage);    \n    useEffect(() => {\n        if(imageLocation !== undefined) {\n            setImage(imageLocation);\n        }\n    },[])\n\n    const cardClickHandler = (ev) => {\n        history.push(clickRoute,{eventData: props.eventData, date: date, time: time, imgLink: image});\n    }\n\n    return (\n        <Card \n            raised={false}\n            variant=\"outlined\"\n            className={classes.card}\n        >\n            <CardActionArea onClick={cardClickHandler}> \n                <CardMedia\n                    className={classes.image}\n                    component=\"img\"\n                    src={image}\n                    title=\"Event Poster\"\n                />\n                <CardContent>\n                    <div className={classes.flexRow} style={{marginBottom: \"5px\"}}>\n                        <Typography\n                            gutterBottom\n                            variant=\"h5\"\n                            component=\"h2\"\n                            className={classes.heading}\n                        >\n                            {heading}\n                        </Typography>\n                        <Chip label={`${genre}`} variant=\"outlined\" color=\"secondary\"/>\n                    </div>\n\n                    <Chip \n                        label={`${time}`} \n                        icon={<ScheduleIcon/>} \n                        color=\"primary\"\n                        className={classes.chip}\n                    />\n                    <Chip \n                        label={`${date}`}\n                        icon={<EventIcon/>}\n                        color=\"primary\"\n                        className={classes.chip}\n                    />\n                    <br/>\n                    <Chip \n                        label={`${stateName}`} \n                        color=\"primary\" \n                        icon={<LocationOnIcon/>} \n                        className={classes.chip}\n                    />    \n\n                </CardContent>\n\n            </CardActionArea>\n\n            <Divider variant=\"middle\"/>\n\n            <CardActions className={classes.flexRow}>\n                    <div\n                        style={{width: \"80%\", padding: \"10px\"}}\n                    >\n                        <Grid container direction=\"row\" justify=\"space-between\" >\n                            <Grid item className={classes.subheading}>\n                                Tickets Booked: \n                            </Grid>\n                            <Grid item className={classes.subheading}>\n                                {bookedTickets} of {totalTickets}\n                            </Grid>\n                        </Grid>\n\n                        <ProgressBar\n                            value={(bookedTickets/totalTickets) * 100}\n                        />\n                    </div>\n                    <strong >\n                        {\n                            (isFree === \"No\") ?\n                            (<span className={classes.priceTag}>\n                                Rs: {price}\n                            </span>) : \n                            (<span className={classes.priceFreeTag}>FREE</span>)\n                        }\n                    </strong>                   \n            </CardActions>\n        </Card>\n    )\n}\n\nexport default EventCard\n","/home/s123/Desktop/project/eventzClient/src/components/cards/CardStyle.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/EventPage.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Header.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Organiser.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Styles.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Description.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Address.js",["340"],"import useStyles from './Styles'\nimport { Container,Card,CardContent,Divider,Typography } from '@material-ui/core'\nimport FiberPinIcon from '@material-ui/icons/FiberPin';\nimport PublicIcon from '@material-ui/icons/Public';\nimport LocationCityIcon from '@material-ui/icons/LocationCity';\nimport LocationOnIcon from '@material-ui/icons/LocationOn';\nimport React from 'react'\n\nfunction Address(props) {\n    const {\n        apartment,\n        street,\n        district,\n        stateName,\n        pincode,\n        country\n    } = props.address;\n    const classes = useStyles();\n    return (\n        <Container className={classes.container}>\n            <Card\n                variant=\"outlined\"\n                raised={false}\n            >\n                <CardContent>\n                    <h3>Address of Event</h3>\n                    <Divider \n                        variant=\"middle\"\n                        className={classes.vmargin}\n                    />\n                    <div>\n                        <div className={classes.flexRow}>\n                            <LocationCityIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {apartment}, {street}\n                            </span>\n                        </div>\n                        <div className={`${classes.flexRow} ${classes.vmargin}`}>\n                            <LocationOnIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {district}, {stateName}\n                            </span>\n                        </div>\n                        <div className={classes.flexRow}>\n                            <FiberPinIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {pincode}\n                            </span>\n                        </div>\n                        <div className={`${classes.flexRow} ${classes.vmargin}`}>\n                            <PublicIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {country}\n                            </span>\n                        </div>\n                    </div> \n                </CardContent> \n            </Card>\n        </Container>\n    )\n}\n\nexport default Address\n","/home/s123/Desktop/project/eventzClient/src/pages/UploadEventPoster.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/UserMenu.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/user/MyEvents.js",["341"],"/home/s123/Desktop/project/eventzClient/src/pages/user/Profile.js",[],"/home/s123/Desktop/project/eventzClient/src/components/inputs/CustomSelect.js",["342"],"import { FormControl, InputLabel, makeStyles, MenuItem, Select } from '@material-ui/core';\nimport React, { useEffect, useState, useContext} from 'react'\nimport {useHistory} from 'react-router'\nimport { ComponentContext } from '../../context/Context';\n\nconst useStyles = makeStyles(theme => ({\n    minWidth: {\n        minWidth: \"150px\"\n    }\n}))\n\nfunction CustomSelect(props) {\n    const {selectData,selectHandler,dataHandler,url,page} = props;\n    const [state,setState] = useState(selectData[0]);\n    const {setFeedback} = useContext(ComponentContext);\n    const classes = useStyles();\n    const history = useHistory();\n    useEffect(() => {\n        let fetchUrl = `${url}?type=${state}&page=${page}`\n        const getData = async () => {\n            const data = await dataHandler(fetchUrl,setFeedback,history);\n            selectHandler(data);\n        }\n        getData();\n    },[state])\n\n    const changeHandler = (e) => {\n        setState(e.target.value);\n    }\n\n    return (\n        <FormControl variant=\"outlined\" className={classes.minWidth}>\n            <InputLabel htmlFor=\"outlined-select-control\">{props.label}</InputLabel>\n            <Select\n                label={props.label}\n                onChange={changeHandler}\n                value={state}\n                inputProps={{\n                    id: 'outlined-select-control'\n                }}\n            >\n                {selectData.map((item) => (<MenuItem key={item} value={item}>{item}</MenuItem>))}\n            </Select>\n        </FormControl>\n    )\n}\n\nexport default CustomSelect\n","/home/s123/Desktop/project/eventzClient/src/pages/user/Styles.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/BookEvents.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/bookEvents.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/user/MyTickets.js",["343"],"/home/s123/Desktop/project/eventzClient/src/routes/AdminRoute.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/admin/AdminLogin.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/admin/MonitorEvents.js",["344"],"/home/s123/Desktop/project/eventzClient/src/helpers/feedbackHandler.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/ContactUs.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/submitFormdata.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/admin/Messages.js",[],"/home/s123/Desktop/project/eventzClient/src/context/DataState.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/admin/Styles.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/fetchData.js",[],"/home/s123/Desktop/project/eventzClient/src/components/dataDisplay/CustomTable.js",[],"/home/s123/Desktop/project/eventzClient/src/components/dataDisplay/TableStyle